{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "build",
  "framework": "vite",
  "devCommand": "npm run dev",
  "rewrites": [
    {
      "source": "/api/proxy/:path*/",
      "destination": "/api/proxy/:path*"
    },
    {
      "source": "!{/_next/,/api/,/assets/,/static/,/favicon.ico}/:path*",
      "destination": "/index.html"
    }
  ]
}
